// Game of Life Spirit - Package Manifest
// Converted to inline format for dol-build compatibility

spirit game_of_life @ 0.1.0

docs "Conway's Game of Life as a DOL Spirit. Cellular automaton with B3/S23 rules."

requires {
    @univrs/std: "^0.8"
}

targets {
    rust: { edition: "2024" }
    wasm: { optimize: true, target: "wasm32-unknown-unknown" }
}

config {
    entry: "src/lib.dol"
    target: wasm32
    features: []
}
