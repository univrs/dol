module tests.trait_relationships @ 1.0.0

/// A simple gen for testing
pub gen SimpleValue {
    has value: string
    has count: i64 = 0

    fun get_value() -> string {
        return this.value
    }
}

/// Trait for display functionality
pub trait Displayable {
    /// Return string representation
    is display() -> string
}

/// Gene that wraps strings
pub gen StringWrapper {
    has value: string

    fun to_string() -> string {
        return this.value
    }
}

